{
  "name": "ox-tic-tac-toe-games",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:package": "turbo run build:package --concurrency 8",
    "dev": "turbo dev",
    "dev:web": "turbo dev  --filter=web",
    "start:web": "turbo start  --filter=web",
    "dev:api": "turbo dev  --filter=api",
    "prisma:generate": "turbo prisma:generate",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "",
    "prepare": "pnpm prisma:generate",
    "clean":"turbo clean"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.1.0",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
